# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore other unneeded files.
doc/
*.swp
*~
.project
.DS_Store
.idea

# Ignore private files
config/initializers/01_smtp_setup.rb
config/initializers/02_env.rb
config/initializers/dropbox_setup.rb
heroku_config.sh

# Ignore files produced by PaperClip
public/system

# Ignore database dump
*.dump
